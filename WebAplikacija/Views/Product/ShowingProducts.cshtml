@model WebAplikacija.Models.ListProductsModel
@{
    ViewBag.Title = "ShowingProducts";
}

<h2>Showing Products</h2>

<p>
    <a asp-action="Create">Create new </a>


</p>

<table class="table">

    <thead>

        <tr>
            <th>

            </th>
            <th></th>

        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model.ProductList)
        {

            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
            </tr>
        }
    </tbody>
</table>

